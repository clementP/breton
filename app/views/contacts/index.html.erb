<h1>Listing contacts</h1>

<table>
  <tr>
    <th>Poste</th>
    <th>Prenom</th>
    <th>Nom</th>
    <th>Email</th>
    <th>Telephone</th>
    <th>Descritpion</th>
    <th>Primary</th>
    <th>Booth</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @contacts.each do |contact| %>
  <tr>
    <td><%= contact.poste %></td>
    <td><%= contact.prenom %></td>
    <td><%= contact.nom %></td>
    <td><%= contact.email %></td>
    <td><%= contact.telephone %></td>
    <td><%= contact.descritpion %></td>
    <td><%= contact.primary_id %></td>
    <td><%= contact.booth_id %></td>
    <td><%= link_to 'Show', contact %></td>
    <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
    <td><%= link_to 'Destroy', contact, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Contact', new_contact_path %>
